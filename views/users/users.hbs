<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="x_panel">
    <div class="page-title">
      <!--#my-modal -->
      <h1>Список пользователей</h1>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <div class="row">
        <div class="col-md-12">
          <!-- Tab panes -->
          <div class="tab-content">
            <div class="row">
              <div class="page-title col-md-12">
                <a href="/users/addusers" role="button" id="">
                  <button class="btn btn-success " style="float: right;">
                    Добавить пользователя
                  </button>
                </a>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <div class="card-box table-responsive">
                  <div id="datatable_wrapper" class="dataTables_wrapper dt-bootstrap no-footer">
                    <div class="row">
                      <div class="col-sm-12">
                        <div class="card-box table-responsive">
                          <table id="data-table" class="table table-striped table-bordered dataTable no-footer"
                            style="width: 100%" role="grid" aria-describedby="datatable_info">
                            <thead>
                              <tr role="row">
                                <th class="sorting_asc" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1"
                                  aria-sort="ascending" aria-label="ID: активировать для сортировки столбца по убыванию"
                                  style="width: 10px; text-align: center">
                                  №
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1"
                                  aria-label="ФИО: активировать для сортировки столбца по убыванию"
                                  style="width: 100px; text-align: center">
                                  Логин
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1"
                                  aria-label="ФИО: активировать для сортировки столбца по убыванию"
                                  style="width: 100px; text-align: center">
                                  Роль
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable" rowspan="1" colspan="1"
                                  aria-label="ФИО: активировать для сортировки столбца по убыванию"
                                  style="width: 300px; text-align: center">
                                  Действия
                                </th>
                              </tr>
                            </thead>
                            <tbody>
                              {{#if users.length}}
                              {{#each users}}
                              <tr role="row" class="odd">
                                <td class="sorting_1" style="
                                    text-align: center;
                                    vertical-align: middle;
                                  ">
                                  {{counter @index}}
                                </td>
                                <td class="sorting_1" style="
                                    text-align: center;
                                    vertical-align: middle;
                                  ">
                                  {{ login }}
                                </td>
                                <td class="sorting_1" style="
                                    text-align: center;
                                    vertical-align: middle;
                                  ">
                                  {{ typeUser }}
                                </td>
                                <td style="text-align: center; padding: 5px 0 0 0">
                                  <a href="/users/{{ _id }}/editusers?allow=true" class="btn btn-primary btn-md"
                                    data-rel="tooltip" title="" data-original-title="Редактировать"><span
                                      class="glyphicon glyphicon-pencil"></span>
                                  </a>
                                  <a href="/users/{{ _id }}/user_delete" class="btn btn-danger btn-md"
                                    data-rel="tooltip" title="" data-original-title="Удалить"><span
                                      class="glyphicon glyphicon-trash" style="color: #fff"></span>
                                  </a>
                                </td>
                              </tr>
                              {{/each}}
                              {{/if}}
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


