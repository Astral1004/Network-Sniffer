    <script>
        $(document).ready(function () {
            App.init();
            TableManageDefault.init();
        });
        
        $('.fa-more').click(function () {
          $(this).toggleClass("fa-arrow-circle-o-left fa-arrow-circle-o-right");
          $('.sidepanel').toggleClass("sidepanel-act");
          $(this).addClass("active");
          $(this).parent("td").addClass("active");
      });

  </script>
<script>
    $(document).ready(function () {
        $('#data-table').DataTable();
        
        $('#table_accesscontrol').DataTable({
            aoColumnDefs: [
                { aTargets: [4], bSortable: false },
            ]
        });
        $('#datatable5').DataTable();
        $('#table_logs').DataTable({
            aoColumnDefs: [
                { aTargets: [3], bSortable: false },
            ]
        });

        $('#journal_apk').DataTable({
            aoColumnDefs: [

                { aTargets: [8, 9], bSortable: false },

            ]
        });
        
        $('#table_complexes').DataTable({
            
            aoColumnDefs: [
                { aTargets: [6], bSortable: false },
            ]
        });
        $('#settings_file_apk__file_info').DataTable({
            "searching": false,
            "bLengthChange": false,
            aoColumnDefs: [
                { aTargets: [0,1], bSortable: false },
            ]
        });
        $('#settings_file_apk__log_info').DataTable({
            aoColumnDefs: [
                { aTargets: [3], bSortable: false },
            ]
        });
        $('#table_result_setting_file_apk').DataTable({
            "searching": false,
            "bLengthChange": false,

            aoColumnDefs: [
                { aTargets: [0, 1], bSortable: false },

            ]
        });
        $('#table_settings_file').DataTable({
            "searching": false,
            "iDisplayLength": 5,
            "bLengthChange": false,
            aoColumnDefs: [
                { aTargets: [3], bSortable: false },
            ]
        });
        $('#table_filehandling').DataTable({
            aoColumnDefs: [
                { aTargets: [2, 3, 4, 5, 6], bSortable: false },

            ]
        });

        $('.AdminAPK').hide();
        $('.AdminComplex').hide();
        

        function clearCheckBox() {
            $('.flat:checked').prop('checked', false);
            $('#flat1').attr('checked', true)
            $('#flat1').attr('disabled', true)
            
                $('.accesscontrol_btn').show();
            
        }

        $('.selectAccess').change(function () {
            let currentIndex = $('.selectAccess')[0].selectedIndex;
            if (currentIndex == 0) {
                clearCheckBox();
                $('.AdminAPK').hide();
                $('.AdminComplex').hide();
                
            }
            if (currentIndex == 1) {
                clearCheckBox();
                $('.AdminAPK').show();
                $('.AdminComplex').hide();
                
            }
            if (currentIndex == 2) {
                clearCheckBox();
                $('.AdminAPK').hide();
                $('.AdminComplex').show();
                
            }
            

        })
    });

</script>

</html>